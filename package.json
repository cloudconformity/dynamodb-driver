{
	"name": "dynamodb-driver",
	"version": "1.9.2",
	"description": "Simple AWS DynamoDB driver for Node.js",
	"main": "index.js",
	"scripts": {
		"check": "npm run lint && npm run test",
		"test": "mocha --reporter spec",
		"lint": "eslint '**/*.js' --ignore-path .gitignore",
		"lint:fix": "npm run lint -- --fix",
		"prepare": "husky install",
		"pre-commit": "lint-staged"
	},
	"keywords": [
		"node",
		"js",
		"aws",
		"dynamodb",
		"driver",
		"wrapper"
	],
	"author": "Xabi Errotabehere",
	"license": "ISC",
	"dependencies": {
		"aws-sdk": "^2.870.0",
		"bluebird": "^3.7.2",
		"uuid": "^8.3.2"
	},
	"devDependencies": {
		"chai": "^3.4.1",
		"chai-as-promised": "^5.1.0",
		"eslint": "^7.3.1",
		"eslint-config-prettier": "^8.3.0",
		"eslint-plugin-node": "^11.1.0",
		"eslint-plugin-prettier": "^3.1.4",
		"eslint-plugin-security": "^1.4.0",
		"husky": "^6.0.0",
		"lint-staged": "^11.0.0",
		"mocha": "^5.2.0",
		"mock-aws": "^1.2.3",
		"mock-aws-sinon": "^1.3.2",
		"prettier": "^2.0.5",
		"proxyquire": "^2.1.0",
		"sinon": "^4.1.3"
	},
	"directories": {
		"test": "test"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/keinu/dynamodb-driver.git"
	},
	"bugs": {
		"url": "https://github.com/keinu/dynamodb-driver/issues"
	},
	"homepage": "https://github.com/keinu/dynamodb-driver#readme",
	"lint-staged": {
		"*.js": [
			"eslint --fix"
		]
	},
	"engines": {
		"node": ">=14.15.0"
	}
}
